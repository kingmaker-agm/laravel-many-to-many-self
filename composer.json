{
    "name": "kingmaker/laravel-many-to-many-self-relationship",
    "type": "library",
    "version": "1.0.0",
    "description": "Laravel Many to Many Relationship on the same Model through a pivot table providing two-way association.",
    "keywords": ["laravel", "relationship", "relation", "many-to-many", "many-to-many-self", "belongs-to-many", "belongs-to-many-self", "two way association"],
    "authors": [
        {
            "name": "Kingmaker",
            "email": "b.g.pratheep.cool@gmail.com",
            "homepage": "https://kingmaker.co.in"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "illuminate/database": ">= 6.20 < 7.0 | >= 7.29 < 8.0"
    },
    "autoload": {
        "psr-4": {
            "Kingmaker\\Illuminate\\Eloquent\\Relations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kingmaker\\Illuminate\\Eloquent\\Relations\\Tests\\": "tests"
        }
    },
    "homepage": "https://github.com/kingmaker-agm/laravel-many-to-many-self",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT",
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0"
    }
}
